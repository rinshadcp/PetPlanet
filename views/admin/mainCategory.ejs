<%- include('./partials/adminHeader.ejs')  %> 
  </head>

  <div class="card mb-5 m-auto mt-5" style="height: 350px; width: 700px">
    <div class="card-body">
      <form
        action="/admin/categoryAdd"
        method="post"
        enctype="multipart/form-data"
      >
        <div class="mb-3">
          <label class="form-label" for="basic-default-fullname"
            >Category</label
          >
          <input
            type="text"
            class="form-control"
            name="category"
            id="basic-default-fullname"
            required
          />
        </div>

        <div class="mt-3">
          <button type="submit" class="btn btn-primary">Send</button>
        </div>
      </form>
    </div>
  </div>
</html>
